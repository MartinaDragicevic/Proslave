      <Text layoutX="812.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Venue Menu">
         <font>
            <Font name="Berlin Sans FB" size="23.0" />
         </font>
      </Text>
      <Text layoutX="221.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Venue Informations">
         <font>
            <Font name="Berlin Sans FB" size="23.0" />
         </font>
      </Text>
    </children>
</AnchorPane>
