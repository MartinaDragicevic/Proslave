                <Cursor fx:constant="HAND" />
            </cursor>
            <font>
                <Font name="Berlin Sans FB" size="14.0" />
            </font>
        </Label>
        <Text layoutX="538.0" layoutY="49.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Venue List">
            <font>
                <Font name="Berlin Sans FB" size="23.0" />
            </font>
        </Text>
      <Button fx:id="acceptButton" layoutX="448.0" layoutY="602.0" mnemonicParsing="false" style="-fx-background-color: green;" text="Accept Venue" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="declineButton" layoutX="601.0" layoutY="602.0" mnemonicParsing="false" style="-fx-background-color: red;" text="Decline Venue" textFill="WHITE">
